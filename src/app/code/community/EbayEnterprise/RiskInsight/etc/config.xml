<config>
	<modules>
		<EbayEnterprise_RiskInsight>
			<version>1.0.0</version>
		</EbayEnterprise_RiskInsight>
	</modules>
	<global>
		<fieldsets>
			<sales_copy_quote_payment>
				<created_at>
					<to_order_payment>created_at</to_order_payment>
				</created_at>
			</sales_copy_quote_payment>
			<sales_convert_quote_payment>
				<created_at>
					<to_order_payment>created_at</to_order_payment>
				</created_at>
			</sales_convert_quote_payment>
		</fieldsets>
		<models>
			<ebayenterprise_riskinsight>
				<class>EbayEnterprise_RiskInsight_Model</class>
				<resourceModel>ebayenterprise_riskinsight_resource</resourceModel>
			</ebayenterprise_riskinsight>
			<ebayenterprise_riskinsight_resource>
				<class>EbayEnterprise_RiskInsight_Model_Resource</class>
				<entities>
					<risk_insight>
						<table>ebayenterprise_risk_insight</table>
					</risk_insight>
				</entities>
			</ebayenterprise_riskinsight_resource>
		</models>
		<helpers>
			<ebayenterprise_riskinsight>
				<class>EbayEnterprise_RiskInsight_Helper</class>
			</ebayenterprise_riskinsight>
		</helpers>
		<blocks>
			<ebayenterprise_riskinsight>
				<class>EbayEnterprise_RiskInsight_Block</class>
			</ebayenterprise_riskinsight>
			<adminhtml>
				<rewrite>
					<sales_order_view>EbayEnterprise_RiskInsight_Block_Adminhtml_Sales_Order_View</sales_order_view>
					<sales_order_view_info>EbayEnterprise_RiskInsight_Block_Adminhtml_Sales_Order_View_Info</sales_order_view_info>
				</rewrite>
			</adminhtml>
		</blocks>
		<resources>
			<ebayenterprise_riskinsight_setup>
				<setup>
					<module>EbayEnterprise_RiskInsight</module>
				</setup>
			</ebayenterprise_riskinsight_setup>
		</resources>
		<events>
			<sales_model_service_quote_submit_after>
				<observers>
					<handle_order_risk_fraud>
						<class>ebayenterprise_riskinsight/observer</class>
						<method>handleSalesModelServiceQuoteSubmitAfter</method>
						<type>singleton</type>
					</handle_order_risk_fraud>
				</observers>
			</sales_model_service_quote_submit_after>
			<checkout_submit_all_after>
				<observers>
					<handle_multi_shipping_orders_risk_fraud>
						<class>ebayenterprise_riskinsight/observer</class>
						<method>handleCheckoutSubmitAllAfter</method>
						<type>singleton</type>
					</handle_multi_shipping_orders_risk_fraud>
				</observers>
			</checkout_submit_all_after>
			<sales_order_save_after>
				<observers>
					<handle_order_final_action_risk_feedback>
						<class>ebayenterprise_riskinsight/observer</class>
						<method>handleSalesOrderSaveAfter</method>
						<type>singleton</type>
					</handle_order_final_action_risk_feedback>
				</observers>
			</sales_order_save_after>
		</events>
	</global>
	<admin>
		<routers>
			<adminhtml>
				<args>
					<modules>
						<EbayEnterprise_RiskInsight after="Mage_Adminhtml">EbayEnterprise_RiskInsight</EbayEnterprise_RiskInsight>
					</modules>
				</args>
			</adminhtml>
		</routers>
	</admin>
	<adminhtml>
		<translate>
			<modules>
				<EbayEnterprise_RiskInsight>
					<files>
						<default>EbayEnterprise_RiskInsight.csv</default>
					</files>
				</EbayEnterprise_RiskInsight>
			</modules>
		</translate>
		<layout>
			<updates>
				<ebayenterprise_riskinsight>
					<file>ebayenterprise_riskinsight.xml</file>
				</ebayenterprise_riskinsight>
			</updates>
		</layout>
	</adminhtml>
	<crontab>
		<jobs>
			<ebayenterprise_riskinsight_process_risk_fraud>
				<schedule><cron_expr>0 */3 * * *</cron_expr></schedule>
				<run>
					<model>ebayenterprise_riskinsight/observer::detectFraudulentOrders</model>
				</run>
			</ebayenterprise_riskinsight_process_risk_fraud>
			<ebayenterprise_riskinsight_resend_fail_feedback_request>
				<schedule><cron_expr>0 */3 * * *</cron_expr></schedule>
				<run>
					<model>ebayenterprise_riskinsight/observer::resendFeedbacks</model>
				</run>
			</ebayenterprise_riskinsight_resend_fail_feedback_request>
		</jobs>
	</crontab>
	<default>
		<ebayenterprise_riskinsight>
			<risk_insight>
				<enabled>0</enabled>
				<store_id/>
				<hostname/>
				<key/>
				<timeout>3000</timeout>
				<debug>0</debug>
				<order_source/>
				<high_action>Hold for Review</high_action>
				<medium_action>Hold for Review</medium_action>
				<low_action>Process</low_action>
				<unknown_action>Hold for Review</unknown_action>
				<language_code>en</language_code>
				<feedback_request_resend_threshold>2</feedback_request_resend_threshold>
				<card_type_map>
					<AE>AM</AE>
					<VI>VC</VI>
					<MC>MC</MC>
					<DI>DC</DI>
					<JCB>OTHER</JCB>
					<SM>OTHER</SM>
					<SO>OTHER</SO>
					<OT>OTHER</OT>
					<cashondelivery>CASH</cashondelivery>
					<paypal_express>PY</paypal_express>
					<payflow_advanced>PY</payflow_advanced>
					<payflow_link>PY</payflow_link>
					<hosted_pro>PY</hosted_pro>
					<verisign>PY</verisign>
					<paypaluk_direct>PY</paypaluk_direct>
					<paypaluk_express>PY</paypaluk_express>
					<paypal_standard>PY</paypal_standard>
					<paypal_direct>PY</paypal_direct>
					<paypal_express_bml>BL</paypal_express_bml>
					<free>OTHER</free>
					<giftcard>GC</giftcard>
					<checkmo>OTHER</checkmo>
					<ogone>OTHER</ogone>
					<purchaseorder>OTHER</purchaseorder>
					<banktransfer>OTHER</banktransfer>
				</card_type_map>
				<payment_adapter_map>
					<authorizenet>ebayenterprise_riskinsight/payment_adapter_authorizenet</authorizenet>
					<giftcard>ebayenterprise_riskinsight/payment_adapter_giftcard</giftcard>
					<checkmo>ebayenterprise_riskinsight/payment_adapter_checkmo</checkmo>
					<ccsave>ebayenterprise_riskinsight/payment_adapter_default</ccsave>
					<payflow_advanced>ebayenterprise_riskinsight/payment_adapter_default</payflow_advanced>
					<payflow_link>ebayenterprise_riskinsight/payment_adapter_default</payflow_link>
					<hosted_pro>ebayenterprise_riskinsight/payment_adapter_default</hosted_pro>
					<paypal_express>ebayenterprise_riskinsight/payment_adapter_paypal_express</paypal_express>
					<verisign>ebayenterprise_riskinsight/payment_adapter_default</verisign>
					<paypaluk_direct>ebayenterprise_riskinsight/payment_adapter_default</paypaluk_direct>
					<paypaluk_express>ebayenterprise_riskinsight/payment_adapter_default</paypaluk_express>
					<paypal_standard>ebayenterprise_riskinsight/payment_adapter_default</paypal_standard>
					<paypal_direct>ebayenterprise_riskinsight/payment_adapter_default</paypal_direct>
					<paypal_express_bml>ebayenterprise_riskinsight/payment_adapter_default</paypal_express_bml>
					<pbridge>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge>
					<pbridge_authorizenet>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_authorizenet>
					<pbridge_ogone_direct>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_ogone_direct>
					<pbridge_paypal_direct>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_paypal_direct>
					<pbridge_paypaluk_direct>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_paypaluk_direct>
					<pbridge_verisign>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_verisign>
					<pbridge_ogone_direct>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_ogone_direct>
					<pbridge_ogone_direct_debit>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_ogone_direct_debit>
					<pbridge_worldpay_direct>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_worldpay_direct>
					<pbridge_cybersource_soap>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_cybersource_soap>
					<pbridge_dibs>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_dibs>
					<pbridge_braintree_basic>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_braintree_basic>
					<pbridge_sagepay_direct>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_sagepay_direct>
					<pbridge_firstdata>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_firstdata>
					<pbridge_eway_direct>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_eway_direct>
					<pbridge_payone_gate>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_payone_gate>
					<pbridge_paybox_direct>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_paybox_direct>
					<pbridge_psigate_basic>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_psigate_basic>
					<pbridge_payone_debit>ebayenterprise_riskinsight/payment_adapter_pbridge</pbridge_payone_debit>
					<ogone>ebayenterprise_riskinsight/payment_adapter_default</ogone>
					<purchaseorder>ebayenterprise_riskinsight/payment_adapter_purchase_order</purchaseorder>
				</payment_adapter_map>
			</risk_insight>
		</ebayenterprise_riskinsight>
	</default>
	<phpunit>
		<suite>
			<modules>
				<EbayEnterprise_RiskInsight/>
			</modules>
			<groups>
				<lib>Lib</lib>
			</groups>
		</suite>
	</phpunit>
</config>
