{
	"name": "ebayenterprise/magento-with-universal-customer-profile",
	"description": "Installation of the eBay Enterprise Risk Insight Extension in Magento",
	"license": "proprietary",
	"type": "project",
	"require": {
		"ebayenterprise/magento-risk-insight": "dev-master",
		"ecomdev/ecomdev_phpunit": "~0.3",
		"pdepend/pdepend": "~1.1",
		"phpdocumentor/phpdocumentor":"~2.4",
		"phploc/phploc": "~2.0",
		"phpmd/phpmd": "~1.5",
		"phpunit/dbunit": "~1",
		"phpunit/php-invoker": "~1.1",
		"phpunit/phpunit-selenium": "~1",
		"phpunit/phpunit-story": "~1",
		"sebastian/phpcpd": "~2.0",
		"squizlabs/php_codesniffer": "~1.5"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.firegento.com"
		},
		{
			"type": "vcs",
			"url": "/git/magento-risk-insight"
		}
	],
	"extra": {
		"magento-deploystrategy": "copy",
		"magento-root-dir": "."
	}
}
